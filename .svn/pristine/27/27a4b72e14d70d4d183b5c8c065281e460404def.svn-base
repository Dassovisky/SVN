@import '_variaveis';

@font-face {
  font-family: 'Material Icons';
  font-style: normal;
  font-weight: 400;
  src: url(https://fonts.gstatic.com/s/materialicons/v29/2fcrYFNaTjcS6g4U3t-Y5ZjZjT5FdEJ140U2DJYC3mY.woff2) format('woff2');
}
.material-icons {
  font-family: 'Material Icons';
  font-weight: normal;
  font-style: normal;
  font-size: 24px;
  line-height: 1;
  letter-spacing: normal;
  text-transform: none;
  display: inline-block;
  white-space: nowrap;
  word-wrap: normal;
  direction: ltr;
  -webkit-font-feature-settings: 'liga';
  -webkit-font-smoothing: antialiased;
  outline: 0;
}

html, body{
    font-family: $font_face, "Arial", sans-serif;
    font-size: 14px;
    font-weight: 400;
    line-height: 20px;
}

@import url('https://fonts.googleapis.com/css?family=Raleway:300,400,700');

textarea, button, a, textarea:hover, button:hover, a:hover, #tt1, #tt1:hover{
    outline: none;
}
.text_center{
    text-align: center;
}
ul:focus, li:focus, a:focus, input:focus, textearea:focus{
    outline: 0;
}